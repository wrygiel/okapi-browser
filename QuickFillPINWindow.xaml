<Window x:Class="OkapiBrowser.QuickFillPINWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Token Authorization" Height="194" Width="346" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" Icon="okapi.ico">
    <Grid Height="145">
        <Label Content="PIN code:" HorizontalAlignment="Left" Margin="24,33,0,0" Name="label1" VerticalAlignment="Top" Width="99" FontSize="16" />
        <TextBox HorizontalAlignment="Left" Margin="127,35,0,0" Name="PIN" VerticalAlignment="Top" Width="156" FontSize="16" Text="" AcceptsReturn="False" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="127,85,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" IsDefault="True" />
    </Grid>
</Window>
